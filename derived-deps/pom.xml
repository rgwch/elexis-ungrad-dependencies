<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <artifactId>ungrad-dependencies</artifactId>
    <groupId>ch.rgw</groupId>
    <version>0.1.0.SNAPSHOT</version>
    <repositories>
        <repository>
            <id>maven.repo.alter</id>
            <name>Mvnrepository</name>
            <url>https://mvnrepository.com/</url>
        </repository>
        <repository>
            <id>central</id>
            <name>Central Repository</name>
            <url>http://repo.maven.apache.org/maven2/</url>
        </repository>
    </repositories>
    <pluginRepositories>
        <pluginRepository>
            <id>reficio</id>
            <url>http://repo.reficio.org/maven/</url>
        </pluginRepository>
    </pluginRepositories>


    <build>
        <plugins>
            <plugin>
                <groupId>org.reficio</groupId>
                <artifactId>p2-maven-plugin</artifactId>
                <version>1.2.0-SNAPSHOT</version>
                <executions>
                    <execution>
                        <id>default-cli</id>
                        <configuration>
                            <artifacts>
                                <!-- specify your depencies here -->
                                <!-- groupId:artifactId:version -->
                                <artifact>
                                    <id>io.vertx:vertx-core:3.2.1</id>
                                </artifact>
                                <artifact>
                                    <id>org.apache.servicemix.bundles:org.apache.servicemix.bundles.zxing:3.1.0_1</id>
                                </artifact>
                                <artifact>
                                    <id>org.odftoolkit:odfdom-java:0.8.6</id>
                                </artifact>
                                <artifact>
                                    <id>com.sun.jna:jna:3.0.9</id>
                                </artifact>
                                <artifact>
                                    <id>jfree:jfreechart-experimental:1.0.9</id>
                                </artifact>
                                <artifact>
                                    <id>junit:junit:4.12</id>
                                </artifact>
                                <artifact>
                                    <id>org.jdom:jdom:1.1.3</id>
                                </artifact>
                                <artifact>
                                    <id>
                                        javax.ws.rs:javax.ws.rs-api:2.0.1
                                    </id>
                                </artifact>
                                <artifact>
                                    <id>com.eclipsesource.jaxrs:consumer:jar:5.3</id>
                                </artifact>
                                <artifact>
                                    <id>ca.uhn.hapi:hapi-osgi-base:jar:1.2</id>
                                </artifact>
                                <artifact>
                                    <id>org.apache.poi:poi:jar:3.14</id>
                                </artifact>
                                <artifact>
                                    <id>uk.org.lidalia:sysout-over-slf4j:jar:1.0.2</id>
                                </artifact>
                                <artifact><id>org.slf4j:jul-to-slf4j:jar:1.7.21</id></artifact>
                                <artifact><id>org.beanshell:bsh:jar:2.0b5</id></artifact>
                                <artifact><id>org.bouncycastle:bcprov-jdk16:jar:1.46</id></artifact>
                                <artifact><id>org.postgresql:postgresql:jar:9.4.1208.jre7</id></artifact>
                                <artifact><id>mysql:mysql-connector-java:jar:5.1.39</id></artifact>
                                <artifact><id>com.h2database:h2:jar:1.4.192</id></artifact>
                                <artifact><id>org.apache.servicemix.bundles:org.apache.servicemix.bundles.commons-dbcp:jar:1.4_3</id></artifact>
                                <artifact><id>au.com.bytecode:opencsv:jar:2.4</id></artifact>
                                <artifact><id>org.apache.commons:commons-jexl:jar:2.1.1</id></artifact>
                                <artifact><id>com.healthmarketscience.jackcess:jackcess:jar:2.1.4</id></artifact>
                                <artifact><id>jfree:jcommon:jar:1.0.16</id></artifact>
                                <artifact><id>org.jfree:jfreechart:jar:1.0.17</id></artifact>
                                <artifact><id>jfree:jfreechart-swt:jar:1.0.9</id></artifact>
                                <artifact><id>com.lowagie:itext:jar:2.0.8</id></artifact>
                            </artifacts>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>


</project>
